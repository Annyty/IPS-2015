<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="VirtualMonkeyPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.inecol.virtualmonkey.entidades.TesisAnexo</class>
    <class>org.inecol.virtualmonkey.entidades.Autor</class>
    <class>org.inecol.virtualmonkey.entidades.Usuario</class>
    <class>org.inecol.virtualmonkey.entidades.Reporte</class>
    <class>org.inecol.virtualmonkey.entidades.Tesis</class>
    <class>org.inecol.virtualmonkey.entidades.DirectorTesis</class>
    <class>org.inecol.virtualmonkey.entidades.Primate</class>
    <class>org.inecol.virtualmonkey.entidades.Disciplina</class>
    <class>org.inecol.virtualmonkey.entidades.Anexo</class>
    <class>org.inecol.virtualmonkey.entidades.Comentario</class>
    <class>org.inecol.virtualmonkey.entidades.Subdisciplina</class>
    <class>org.inecol.virtualmonkey.entidades.Pais</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/virtualmonkey"/>
      <property name="javax.persistence.jdbc.user" value="inecol"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="inecol"/>
    </properties>
  </persistence-unit>
</persistence>
